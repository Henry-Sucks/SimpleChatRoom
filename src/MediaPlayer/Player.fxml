<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="playerPane" prefHeight="675.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MediaPlayer.PlayerController">
   <children>
      <AnchorPane fx:id="playerHolder" layoutX="274.0" layoutY="468.0" prefHeight="208.0" prefWidth="630.0">
         <children>
            <Label fx:id="runtimeLabel" layoutX="14.0" layoutY="89.0" style="-fx-background-color: rgba(255, 255, 255, 0);" text="Label">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label fx:id="volumeLabel" layoutX="468.0" layoutY="99.0" style="-fx-background-color: rgba(255, 255, 255, 0);" text="Label">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Slider fx:id="songProgressBar" layoutY="53.0" prefHeight="38.0" prefWidth="628.0" style="-fx-background-color: rgba(255, 255, 255, 0);" />
            <Slider fx:id="volumeBar" layoutX="468.0" layoutY="127.0" style="-fx-background-color: rgba(255, 255, 255, 0);" />
            <Label fx:id="songNameLabel" layoutX="11.0" layoutY="1.0" prefHeight="54.0" prefWidth="618.0" style="-fx-background-color: rgba(255, 255, 255, 0);" text="Label">
               <font>
                  <Font name="Microsoft YaHei" size="24.0" />
               </font></Label>
            <HBox layoutX="209.0" layoutY="108.0" prefHeight="66.0" prefWidth="140.0">
               <children>
                  <Button fx:id="prevBtn" mnemonicParsing="false" onAction="#prevClick" style="-fx-background-color: rgba(255, 255, 255, 0);" />
                  <Button fx:id="playBtn" mnemonicParsing="false" onAction="#playClick" style="-fx-background-color: rgba(255, 255, 255, 0);" />
                  <Button fx:id="nextBtn" mnemonicParsing="false" onAction="#nextClick" style="-fx-background-color: rgba(255, 255, 255, 0);" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <ListView fx:id="playlistView" layoutX="-6.0" prefHeight="676.0" prefWidth="280.0" />
      <ListView fx:id="songListView" layoutX="274.0" layoutY="53.0" prefHeight="414.0" prefWidth="628.0" />
      <Label fx:id="playlistLabel" alignment="BASELINE_CENTER" layoutX="272.0" prefHeight="54.0" prefWidth="628.0" style="-fx-background-color: rgba(255, 255, 255, 0.5);" text="Label">
         <font>
            <Font name="Microsoft YaHei" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
